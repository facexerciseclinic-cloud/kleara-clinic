{
  "name": "kleara-clinic-management",
  "version": "1.0.0",
  "private": true,
  "description": "Comprehensive Aesthetic Clinic Management System for Kleara Clinic",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "npm run dev --workspace=server",
    "client": "npm start --workspace=client",
    "build": "npm run build --workspace=client",
    "start": "npm start --workspace=server",
    "install-all": "npm install",
    "test:server": "mocha --recursive server/test --exit"
  },
  "workspaces": [
    "server",
    "client"
  ],
  "keywords": [
    "clinic",
    "management",
    "healthcare",
    "aesthetic",
    "CRM"
  ],
  "author": "Kleara Clinic Development Team",
